{
  "manifest_version": 3,
  "name": "CS2 HLTV Matches",
  "version": "1.71",
  "description": "Displays live and upcoming professional Counter-Strike 2 matches.",
  "permissions": ["storage", "alarms"],
  "host_permissions": ["https://teemo.uk/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
