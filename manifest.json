{
    "manifest_version": 3,
    "name": "HLTV Matches",
    "version": "1.0",
    "description": "Displays matches of the day from HLTV",
    "permissions": ["storage", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  